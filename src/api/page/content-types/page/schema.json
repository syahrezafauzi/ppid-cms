{
  "kind": "collectionType",
  "collectionName": "pages",
  "info": {
    "singularName": "page",
    "pluralName": "pages",
    "displayName": "Page"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "excludePanel": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[\"Left\", \"Right\"]",
      "options": [
        "Left",
        "Right"
      ],
      "required": false
    },
    "url": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "pdf": {
      "type": "component",
      "component": "page.pdf",
      "repeatable": true
    },
    "link": {
      "type": "component",
      "component": "widget.link",
      "repeatable": true
    },
    "other": {
      "type": "dynamiczone",
      "components": [
        "page.dip"
      ]
    }
  }
}
